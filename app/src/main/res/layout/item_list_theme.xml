<?xml version="1.0" encoding="utf-8"?>
<dmitriy.deomin.aimpradioplalist.custom.Fon_item xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fon_item_list_theme"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="5dp"
    android:layout_marginTop="2dp"
    android:layout_marginRight="5dp"
    android:layout_marginBottom="2dp"
    android:alpha="0.8"
    android:background="@null"
    android:elevation="0dp"
    app:cardBackgroundColor="@null"
    app:cardCornerRadius="10dp"
    app:cardElevation="3dp"
    app:cardPreventCornerOverlap="false"
    tools:targetApi="lollipop">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <dmitriy.deomin.aimpradioplalist.custom.Btn
            android:id="@+id/name_thme"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text="Button" />
    </LinearLayout>
</dmitriy.deomin.aimpradioplalist.custom.Fon_item>
